{
  "name": "unopinionated-angular-toolbox",
  "version": "0.2.6",
  "author": "Thomas Elliott",
  "license": "MIT",
  "description": "Unopinionated Angular Toolbox",
  "private": false,
  "main": "./dist/unopinionated-angular-toolbox.js",
  "typings": "./dist/unopinionated-angular-toolbox.d.ts",
  "repository": {
  "type": "git",
  "url": "git://github.com/tme321/Unopinionated-Angular.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildLib": "ng-packagr -p ng-package.json",
    "buildDemo": "ng build --prod -op \"./docs/demo\"",
    "noJekyll": "touch ./docs/.nojekyll",
    "genDoc": "./node_modules/.bin/compodoc -p src/tsconfig.compodoc.json -d \"./docs\" && npm run noJekyll &&  npm run buildDemo",
    "clean": "rm -r ./dist && rm -r ./docs"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@compodoc/compodoc": "^1.0.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.0.0-rc.9",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
